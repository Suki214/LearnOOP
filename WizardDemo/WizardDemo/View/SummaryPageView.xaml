<UserControl x:Class="WizardDemo.View.SummaryPageView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WizardDemo.View"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <GroupBox Header="The following will be migrated:" Margin="5,10,5,10">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                </Grid.RowDefinitions>
                <Label Content="User and Profiles:" Grid.Row="0" Margin="5,10,5,10"></Label>
                <Label Content="Serilization:" Grid.Row="1" Margin="5,10,5,10"></Label>
                <Label Content="Others:" Grid.Row="2" Margin="5,10,5,10"></Label>
            </Grid>
        </GroupBox>
    </Grid>
</UserControl>
