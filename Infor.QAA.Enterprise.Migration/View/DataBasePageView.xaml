                <TextBox Name="txtVisionUser" Grid.Column="1" Grid.ColumnSpan="2" Height="23" HorizontalAlignment="Left" Margin="30,83,0,0" VerticalAlignment="Top" Width="142" Grid.Row="1" Text="sa" Style="{StaticResource textbox-ux3}"/>
                <Label Content="Password" Height="28" HorizontalAlignment="Left" Margin="6,130,0,0" Name="label4" VerticalAlignment="Top" Grid.Row="1" Grid.ColumnSpan="2" Width="62" />
                <PasswordBox Name="txtVisionPassword" Grid.Column="1" Grid.ColumnSpan="2" Grid.Row="1" Height="23" HorizontalAlignment="Left" Margin="30,130,0,0" VerticalAlignment="Top" Width="142" Password="Password" />
            </Grid>
        </GroupBox>
        <GroupBox Header="QAA Evlove" Height="233" HorizontalAlignment="Left" Margin="265,25,0,0" Name="groupBox2" VerticalAlignment="Top" Width="230">
            <Grid Height="206" Width="222">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="44*" />
                    <ColumnDefinition Width="62*" />
                    <ColumnDefinition Width="117*" />
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="8*" />
                    <RowDefinition Height="119*" />
                </Grid.RowDefinitions>
                <Label Content="Machine" Grid.ColumnSpan="2" Grid.Row="1" Height="28" HorizontalAlignment="Left" Margin="6,0,0,0" Name="label5" VerticalAlignment="Top" Width="62" />
                <TextBox Name="txtQaaMachine" Grid.Column="1" Grid.ColumnSpan="2" Grid.Row="1" Height="23" HorizontalAlignment="Left" Margin="30,2,0,0" VerticalAlignment="Top" Width="142" Text="shqaacqc02" Style="{StaticResource textbox-ux3}"/>
                <Label Content="Database" Grid.ColumnSpan="2" Grid.Row="1" Height="28" HorizontalAlignment="Left" Margin="6,37,0,0" Name="label6" VerticalAlignment="Top" Width="62" />
