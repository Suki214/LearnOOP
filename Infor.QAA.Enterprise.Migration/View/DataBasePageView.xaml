             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" >
    <Grid>
        <GroupBox Header="Vision10.x" Height="233" HorizontalAlignment="Left" Margin="26,23,0,0" Name="groupBox1" VerticalAlignment="Top" Width="230">
            <Grid Height="206" Width="222">
                <Grid.RowDefinitions>
                    <RowDefinition Height="13*" />
                    <RowDefinition Height="193*" />
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="44*" />
                    <ColumnDefinition Width="62*" />
                    <ColumnDefinition Width="117*" />
                </Grid.ColumnDefinitions>
                <Label Content="Machine" Height="28" HorizontalAlignment="Left" Margin="6,0,0,0" Name="label1" VerticalAlignment="Top" Grid.Row="1" Grid.ColumnSpan="2" Width="62" />
                <TextBox Name="txtVisionMachine" Height="23" HorizontalAlignment="Left" Margin="30,2,0,0" VerticalAlignment="Top" Width="142" Grid.Column="1" Grid.ColumnSpan="2" Grid.Row="1" Text="shqaacqc01" Style="{StaticResource textbox-ux3}"/>
                <Label Content="Database" Height="28" HorizontalAlignment="Left" Margin="6,37,0,0" Name="label2" VerticalAlignment="Top" Grid.Row="1" Grid.ColumnSpan="2" Width="62" />
