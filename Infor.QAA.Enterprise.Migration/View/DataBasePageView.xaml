                <Label Content="User" Grid.Row="1" Height="28" HorizontalAlignment="Left" Margin="6,81,0,0" Name="label7" VerticalAlignment="Top" />
                <TextBox Name="txtQaaUser" Grid.Column="1" Grid.ColumnSpan="2" Grid.Row="1" Height="23" HorizontalAlignment="Left" Margin="30,83,0,0" VerticalAlignment="Top" Width="142" Text="sa" Style="{StaticResource textbox-ux3}"/>
                <Label Content="Password" Grid.ColumnSpan="2" Grid.Row="1" Height="28" HorizontalAlignment="Left" Margin="6,130,0,0" Name="label8" VerticalAlignment="Top" Width="62" />
                <PasswordBox Name="txtQaaPassword" Grid.Column="1" Grid.ColumnSpan="2" Grid.Row="1" Height="23" HorizontalAlignment="Left" Margin="30,130,0,0" VerticalAlignment="Top" Width="142" Password="Password" />
            </Grid>
        </GroupBox>
    </Grid>
</UserControl>
